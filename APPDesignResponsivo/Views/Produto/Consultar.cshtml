
@{
    ViewData["Title"] = "Consultar";
    Layout = "~/Views/Shared/_Layout.cshtml";
}

<header class="intro-header" style="background-image: url('/img/post-bg.jpg')">
        <div class="container">
            <div class="row">
                <div class="col-lg-8 col-lg-offset-2 col-md-10 col-md-offset-1">
                    <div class="page-heading">
                        <h1>@Model.Designacao</h1>                     
                    </div>
                </div>
            </div>
        </div>
    </header>
    <!-- Post Content -->
    <article>
        <div class="container">
            <div class="row">
                <div class="col-lg-8 col-lg-offset-2 col-md-10 col-md-offset-1">
                    <p>Preco : @Model.PUnitario </p>
                     <p>Sotck : @Model.StkAtual</p>
                   
                    <p><a href="/Produto/Editar/@Model.Id"> Editar</a></p>
                </div>
            </div>
        </div>
    </article>
    
    <hr>

